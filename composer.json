{
    "name": "ziphp/ziphp",
    "description": "Ziphp Framework",
    "keywords": [
        "ziphp",
        "framework"
    ],
    "homepage": "https://github.com/ziphp/ziphp",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "https://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "support": {
        "issues": "https://github.com/ziphp/ziphp/issues?state=open",
        "source": "https://github.com/ziphp/ziphp"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*",
        "ziphp/zipper": "dev-main",
        "ezyang/htmlpurifier": "^4.16",
        "cebe/markdown": "~1.0.0 | ~1.1.0 | ~1.2.0",
        "bower-asset/jquery": "3.7.*@stable | 3.6.*@stable | 3.5.*@stable | 3.4.*@stable | 3.3.*@stable | 3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
        "bower-asset/inputmask": "^5.0.8 ",
        "bower-asset/punycode": "^2.2",
        "bower-asset/yii2-pjax": "~2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "cebe/indent": "~1.0.2",
        "friendsofphp/php-cs-fixer": "^3.9.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\": "framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ziphp/zipper": true
        },
        "sort-packages": true
    },
    "bin": [
        "framework/yii"
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "2.0.x-dev"
        }
    },
    "scripts": {
        "redo-install": [
            "rm -f composer.lock",
            "@composer install -vvv"
        ]
    }
}
